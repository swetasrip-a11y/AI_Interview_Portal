### Backend environment example
# To use MySQL (XAMPP), set DB_TYPE=mysql and configure MySQL credentials below
DB_TYPE=sqlite
# If using sqlite (default), you can specify DB_PATH
# DB_PATH=./interview_portal.db

# If using MySQL, set these values:
MYSQL_HOST=localhost
MYSQL_USER=root
MYSQL_PASSWORD=
MYSQL_DATABASE=interview_portal

# Server
PORT=5000
# Murf AI Configuration
MURF_API_KEY=your_murf_api_key_here

# Database
DATABASE_URL=./database.db

# Server
PORT=5000
NODE_ENV=development

# JWT
JWT_SECRET=your_jwt_secret_key

# Frontend URL
FRONTEND_URL=http://localhost:3001
